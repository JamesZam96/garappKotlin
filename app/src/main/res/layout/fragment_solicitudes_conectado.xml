           android:textColor="@color/white"
           android:textSize="20sp"
           android:textStyle="bold"
           app:cornerRadius="5sp"
           app:layout_constraintEnd_toEndOf="@id/solicides"
           app:layout_constraintStart_toStartOf="@id/solicides"
           app:layout_constraintTop_toTopOf="@id/Request_4"
           />
       <Button
           android:id="@+id/Request_6"
           android:layout_width="350dp"
           android:layout_height="50dp"
           android:layout_marginTop="60dp"
           android:backgroundTint="#FA3D3B"
           android:text="Solicitud 6"
           android:textColor="@color/white"
           android:textSize="20sp"
           android:textStyle="bold"
           app:cornerRadius="5sp"
           app:layout_constraintEnd_toEndOf="@id/solicides"
           app:layout_constraintStart_toStartOf="@id/solicides"
           app:layout_constraintTop_toTopOf="@id/Request_5"
           />
       <Button
           android:id="@+id/Request_7"
           android:layout_width="350dp"
           android:layout_height="50dp"
           android:layout_marginTop="60dp"
           android:backgroundTint="#FA3D3B"
           android:text="Solicitud 7"
           android:textColor="@color/white"
           android:textSize="20sp"
           android:textStyle="bold"
           app:cornerRadius="5sp"
           app:layout_constraintEnd_toEndOf="@id/solicides"
           app:layout_constraintStart_toStartOf="@id/solicides"
           app:layout_constraintTop_toTopOf="@id/Request_6"
           />
       <Button
           android:id="@+id/Request_8"
           android:layout_width="350dp"
           android:layout_height="50dp"
           android:layout_marginTop="60dp"
           android:backgroundTint="#FA3D3B"
