<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".FragmentProfits"
    android:background="#C1C1C1">


    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/layout"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <Button
            android:id="@+id/October1"
            android:layout_width="350dp"
            android:layout_height="50dp"
            android:backgroundTint="#FA3D3B"
            android:text="1 Octubre"
            android:layout_marginTop="80dp"
            android:textColor="@color/white"
            android:textSize="20sp"
            android:textStyle="bold"
            app:cornerRadius="5sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <Button
            android:id="@+id/October2"
            android:layout_width="350dp"
            android:layout_height="50dp"
            android:text="2 Octubre"
            android:layout_marginTop="20dp"
            android:textColor="@color/white"
            android:textSize="20sp"
            android:textStyle="bold"
            app:cornerRadius="5sp"
            android:backgroundTint="#FA3D3B"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/October1"
            />

        <Button
            android:id="@+id/October3"
            android:layout_width="350dp"
            android:layout_height="50dp"
            android:text="3 Octubre"
            android:layout_marginTop="20dp"
            android:textColor="@color/white"
            android:textSize="20sp"
            android:textStyle="bold"
            app:cornerRadius="5sp"
            android:backgroundTint="#FA3D3B"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/October2"/>

        <Button
            android:id="@+id/October4"
            android:layout_width="350dp"
            android:layout_height="50dp"
            android:text="4 Octubre"
            android:layout_marginTop="20dp"
            android:textColor="@color/white"
            android:textSize="20sp"
            android:textStyle="bold"
            app:cornerRadius="5sp"
            android:backgroundTint="#FA3D3B"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/October3"/>

        <Button
            android:id="@+id/October5"
            android:layout_width="350dp"
            android:layout_height="50dp"
            android:text="5 Octubre"
            android:layout_marginTop="20dp"
            android:textColor="@color/white"
            android:textSize="20sp"
            android:textStyle="bold"
            app:cornerRadius="5sp"
            android:backgroundTint="#FA3D3B"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/October4"/>

        <Button
            android:id="@+id/October6"
            android:layout_width="350dp"
            android:layout_height="50dp"
            android:text="6 Octubre"
            android:layout_marginTop="20dp"
            android:textColor="@color/white"
            android:textSize="20sp"
            android:textStyle="bold"
            app:cornerRadius="5sp"
            android:backgroundTint="#FA3D3B"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/October5"/>

        <Button
            android:id="@+id/October7"
            android:layout_width="350dp"
            android:layout_height="50dp"
            android:text="7 Octubre"
            android:layout_marginTop="20dp"
            android:textColor="@color/white"
            android:textSize="20sp"
            android:textStyle="bold"
            app:cornerRadius="5sp"
            android:backgroundTint="#FA3D3B"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/October6"/>

        <Button
            android:id="@+id/October8"
            android:layout_width="350dp"
            android:layout_height="50dp"
            android:text="8 Octubre"
            android:layout_marginTop="20dp"
            android:textColor="@color/white"
            android:textSize="20sp"
            android:textStyle="bold"
            app:cornerRadius="5sp"
            android:backgroundTint="#FA3D3B"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/October7"/>
    </androidx.constraintlayout.widget.ConstraintLayout>

</FrameLayout>